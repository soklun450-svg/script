local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/tlredz/Library/refs/heads/main/redz-V5-remake/main.luau"))()

local Window = Library:MakeWindow({
  Title = "SLN Hub",
  SubTitle = "SELENA GAMING",
  ScriptFolder = "redz-library-V5"
})

local Minimizer = Window:NewMinimizer({
  KeyCode = Enum.KeyCode.LeftControl
})

local MobileButton = Minimizer:CreateMobileMinimizer({
  Image = "rbxassetid://0",
  BackgroundColor3 = Color3.fromRGB(0, 0, 0)
})

local Tab = Window:MakeTab({
    Name = "Farm",
    Icon = "rbxassetid://4483345998"
})

-- args
local args = {
    workspace:WaitForChild("World")
        :WaitForChild("World2")
        :WaitForChild("Area7")
        :WaitForChild("Exercise")
        :WaitForChild("\232\140\132\229\173\144")
}

-- state
getgenv().AutoExercise = false

Tab:AddToggle({
    Name = "Auto Farm",
    Default = false,
    Callback = function(Value)
        getgenv().AutoExercise = Value

        if Value then
            task.spawn(function()
                while task.wait() do
                    if not getgenv().AutoExercise then break end

                    game:GetService("ReplicatedStorage")
                        :WaitForChild("Remote")
                        :WaitForChild("Event")
                        :WaitForChild("Game")
                        :WaitForChild("[C-S]PlayerExercise")
                        :FireServer(unpack(args))
                end
            end)
        end
    end
})
